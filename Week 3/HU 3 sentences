function setup() {
  createCanvas(800, 400);
}

function draw() {
  background(220, 213, 135);
textStyle( BOLDITALIC)
  textFont('Courier New', 32) 
text('Harrisburg University', 50, 50)         
  textStyle(NORMAL)
  textFont('Courier New', 14)
  text("HU has been an amazing opportunity for me so far. Ive learned alot and have met alot of amazing people that I can call my friends. I'm looking forward to seeing what HU has in store and where it will take me in the future.",100, 100, 700)
}